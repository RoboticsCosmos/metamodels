{
  "@context": {
    "constraints": "https://roboticscosmos.github.io/metamodels/relations/constraints#",

    "Constraint": "constraints:Constraint",
    "DistanceConstraint": "constraints:DistanceConstraint",
    "PoseConstraint": "constraints:PoseConstraint",
    "VelocityConstraint": "constraints:VelocityConstraint",
    "VelocityTwistConstraint": "constraints:VelocityTwistConstraint",
    "AccelerationConstraint": "constraints:AccelerationConstraint",
    "ForceConstraint": "constraints:ForceConstraint",

    "LessThan": "constraints:LessThan",
    "LessThanOrEqual": "constraints:LessThanOrEqual",
    "Equal": "constraints:Equal",
    "GreaterThanOrEqual": "constraints:GreaterThanOrEqual",
    "GreaterThan": "constraints:GreaterThan",
    "NotEqual": "constraints:NotEqual",
    "InInterval": "constraints:InInterval",

    "threshold": {
      "@id": "constraints:threshold",
      "@type": "@id"
    },

    "operator": {
      "@id": "constraints:operator",
      "@type": "@id"
    },

    "quantity": {
      "@id": "constraints:quantity",
      "@type": "@id"
    }
  }
}